{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@actions/*": [
        "./src/actions/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/lib/app/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@scripts/*": [
        "./src/scripts/*"
      ],
      "@auth/*": [
        "./src/lib/auth/*"
      ],
      "@profile/*": [
        "./src/lib/profile/*"
      ],
      "@user/*": [
        "./src/lib/user/*"
      ]
    }
  }
}
